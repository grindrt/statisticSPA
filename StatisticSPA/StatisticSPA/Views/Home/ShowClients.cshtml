
  <table><tr><td height="10px"></td></tr></table>
  <table cellpadding="4" cellspacing="4" width="90%" align="center"
       style="background-color: skyblue; border: solid 2px red; padding-top: 20px;">
  <thead>
    <tr style="background-color: #F5F5F5;">
      <th>Client ID</th>
      <th>First Name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Birth date</th>
      <th>City</th>
      <th>Gender</th>
      <th>Groups</th>
      <th></th>
      <th></th>
    </tr>
  </thead>  
  <tbody>
    <tr data-ng-repeat="Client in Clients">
      <td>{{Client.id}}</td>
      <td>{{Client.firstName}}</td>
      <td>{{Client.lastname}}</td>
      <td>{{Client.email}}</td>
      <td>{{Client.birthDate | date:'yyyy-MM-dd'}}</td>
      <td>{{Client.city}}</td>
      <td>{{Client.gender}}</td>
      <td>
        <table>
          <tr data-ng-repeat="Group in Client.group">
            <td>{{Group.title}}</td>
          </tr>
        </table>
      </td>
      <td><a ng-click="editClient(Client.id)">Edit</a></td>
      <td><a ng-click="deleteClient(Client.id)">Delete</a></td>
    </tr>
  </tbody>
</table>
